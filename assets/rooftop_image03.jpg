				if($(this).offset().top - win_height  <= position - 100) {
					$(this).find('.block-title').addClass('slide-from-left');
					$(this).find('.block-link').addClass('slide-from-right');
					$(this).find('.reservation').addClass('slide-from-top');
					$(this).parent().find('.down').addClass('vertical-bounce');
					var e = $(this);
					setTimeout(function(){ e.parent().find('.down').fadeOut() }, 1500);
				}
			});
		});
		$('.banner-scroll').click(function() {
